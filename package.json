{
  "name": "you-always-were-js",
  "version": "0.1.0",
  "description": "A game prototype using Tauri and TailwindCSS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "check:quality": "npm run type-check && npm run lint && npm run format:check",
    "check:quick": "npm run type-check && echo Quick check passed!",
    "kuzu:bootstrap": "node ./scripts/bootstrapKuzuWasm.mjs"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@langchain/core": "^1.0.1",
    "@langchain/google-genai": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@tauri-apps/api": "^2.1.1",
    "kuzu-wasm": "^0.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
